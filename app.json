{
  "expo": {
    "name": "unlockPI",
    "slug": "unlockPI",
    "version": "1.0.0",
    "scheme": "learn-expo",
    "orientation": "portrait",
    "icon": "./splashscreen.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./unlockpi.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.likitha.n.unlockPI"
    },
    "web": {
      "favicon": "./splashscreen.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c9c1d19a-a587-4fa2-8902-2df190d4f597"
      }
    }
  }
}
